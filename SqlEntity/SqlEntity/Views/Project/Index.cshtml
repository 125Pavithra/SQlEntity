@model IEnumerable<ProjectTbl>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Project</h4>
    <hr />
    <table border="1" cellpadding="0" cellspacing="0">
        <tr>
            <th>ProjectId</th>
            <th>ProjectName</th>
            <th>CreatedBy</th>
            <th>CreatedOn</th>
        </tr>
        @foreach (ProjectTbl Pro in Model)
        {
            <tr>
                <td>@Pro.ProjectId</td>
                <td>@Pro.ProjectName</td>
                <td>@Pro.CreatedBy</td>
                <td>@Pro.CreatedOn</td>
            </tr>
        }
    </table>
</body>
</html>